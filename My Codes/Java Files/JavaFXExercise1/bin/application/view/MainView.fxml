<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane minHeight="300.0" minWidth="300.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.MainViewController">
   <children>
      <Label layoutX="94.0" layoutY="35.0" text="Arithmetic Calculator" />
      <Button layoutX="155.0" layoutY="80.0" minHeight="30.0" minWidth="30.0" mnemonicParsing="false" onAction="#multiply" text="*" />
      <TextField fx:id="input1" layoutX="29.0" layoutY="171.0" prefHeight="26.0" prefWidth="86.0" />
      <TextField fx:id="input2" layoutX="185.0" layoutY="171.0" prefHeight="26.0" prefWidth="79.0" />
      <Label fx:id="label" layoutX="124.0" layoutY="218.0" minHeight="10.0" minWidth="50.0" />
      <Button layoutX="109.0" layoutY="80.0" minHeight="30.0" minWidth="30.0" mnemonicParsing="false" onAction="#subtract" text="-" />
      <Button layoutX="109.0" layoutY="126.0" minHeight="30.0" minWidth="30.0" mnemonicParsing="false" onAction="#add" text="+" />
      <Button layoutX="155.0" layoutY="126.0" minHeight="30.0" minWidth="30.0" mnemonicParsing="false" onAction="#divide" text="/" />
      <Button layoutX="45.0" layoutY="242.0" mnemonicParsing="false" onAction="#openHistory" text="History" />
      <Button fx:id="closeButton" layoutX="212.0" layoutY="242.0" mnemonicParsing="false" onAction="#openTrigonometric" text="Trigo" />
   </children>
</AnchorPane>
